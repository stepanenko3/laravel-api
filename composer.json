{
    "name": "stepanenko3/laravel-logic-containers",
    "description": "Laravel services architecture",
    "keywords": [
        "laravel",
        "services",
        "microservices",
        "micro services",
        "containers",
        "logic",
        "business logics"
    ],
    "homepage": "https://github.com/stepanenko3/laravel-logic-containers",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Stepanenko",
            "email": "stpnnk.artem@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "spatie/laravel-package-tools": "^1.14.1",
        "stepanenko3/laravel-helpers": "^1.2",
        "stepanenko3/laravel-pagination": "^1.0",
        "spatie/laravel-translatable": "^6.4",
        "spatie/laravel-medialibrary": "^10.7",
        "spatie/opening-hours": "^2.13",
        "jaybizzle/crawler-detect": "^1.2",
        "propaganistas/laravel-phone": "^5.0"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^0.17.0",
        "laravel/pint": "^1.7",
        "rector/rector": "^0.15.21"
    },
    "autoload": {
        "psr-4": {
            "Stepanenko3\\LaravelLogicContainers\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stepanenko3\\LaravelLogicContainers\\LaravelLogicContainersProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
